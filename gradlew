#!/usr/bin/env sh

##############################################################################
#
#   Gradle start up script for POSIX generated by Gradle.
#
##############################################################################

APP_HOME=$(dirname "$0")

# Resolve JAVA from org.gradle.java.home in gradle.properties if present
JAVA_EXE=java
JAVA_HOME_PROP=$(grep -m 1 '^org.gradle.java.home=' "$APP_HOME/gradle.properties" 2>/dev/null | cut -d'=' -f2-)
if [ -n "$JAVA_HOME_PROP" ]; then
  JAVA_EXE="$JAVA_HOME_PROP/bin/java"
fi

CLASSPATH=$APP_HOME/gradle/wrapper/gradle-wrapper.jar

exec "$JAVA_EXE" -Dorg.gradle.appname="$0" -classpath "$CLASSPATH" org.gradle.wrapper.GradleWrapperMain "$@"
#!/usr/bin/env sh

